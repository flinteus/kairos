{
  "manifest_version": 3,
  "name": "Kairos",
  "version": "1.0",
  "description": "Генератор паролей",
  "action": {
    "default_popup": "HTML.html",
    "default_icon": {
      "16": "icons/Kairosicon16.png",
      "48": "icons/Kairosicon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/Kairosicon16.png",
    "48": "icons/Kairosicon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["clipboardWrite"]
}